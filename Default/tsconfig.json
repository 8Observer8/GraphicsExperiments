{
    "compilerOptions": {
        "allowJs": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "charset": "utf-8",
        "listFiles": true,
        "locale": "en-us",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "outDir": "Build/",
        "removeComments": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "target": "es3",
        "module": "none"
    },
    "files": [
        "Scripts/default.ts",
        "Scripts/main.ts"
    ]
}