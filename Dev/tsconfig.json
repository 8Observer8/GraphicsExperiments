{
    "compilerOptions": {
        "allowJs": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "charset": "utf-8",
        "listFiles": true,
        "locale": "en-us",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "outFile": "Build/dev.js",
        "removeComments": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "target": "es3",
        "module": "none"
    },
    "files": [
        "Scripts/shaders.ts"
    ]
}