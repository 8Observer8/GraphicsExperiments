{
    "compilerOptions": {
        "allowJs": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "charset": "utf-8",
        "listFiles": true,
        "locale": "en-us",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "outFile": "Build/Math.js",
        "removeComments": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "target": "es3",
        "module": "none"
    },
    "files": [
        "Scripts/Vec2.ts",
        "Scripts/Vec3.ts",
        "Scripts/Vec4.ts",
        "Scripts/Mat2.ts",
        "Scripts/Mat3.ts",
        "Scripts/Mat4.ts"
    ]
}